<h5 class="container" >
  descargar datos en un :
  <%= link_to "CSV", leads_path(format: "csv") %> o 
  <%= link_to "Excel", leads_path(format: "xls") %>
</h5>


<h1 class="container">Listing Personal</h1>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Stage</th>
      <th colspan="3"> Configuraciones</th>
    </tr>
  </thead>

  <tbody>
    <% @leads.each do |lead| %>

      <tr>
        <td><%= lead.name %></td>
        <td><%= lead.email %></td>
        <td><%= lead.stage.name %></td>
        <td><%= link_to 'Show', lead %>
        <%= link_to 'Edit', edit_lead_path(lead) %>
        <%= link_to 'Destroy', lead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

